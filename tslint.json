{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "no-string-literal": false,
        "no-console": [
            false,
            "log"
        ],
        "object-literal-sort-keys":false,
        "ter-indent": [
            true,
            4
        ],
        "max-line-length": [240],
        "no-trailing-whitespace": ["ignore-whitespace"]
    },
    "rulesDirectory": []
}